@page
@model PlanteeaShop.Pages.ProductOrigins.IndexModel

@{
    ViewData["Title"] = "Shop";
}

<h1>Product Origin List</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProductOrigin[0].OriginName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ProductOrigin) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OriginName)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ID">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ID">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
