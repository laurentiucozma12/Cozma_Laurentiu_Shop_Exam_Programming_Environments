@page
@model PlanteeaShop.Pages.ProductOrigins.CreateModel

@{
    ViewData["Title"] = "Shop";
}

<h1>Create Product Origin</h1>

<h4>ProductOrigin</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group mb-3">
                <label asp-for="ProductOrigin.OriginName" class="control-label"></label>
                <input asp-for="ProductOrigin.OriginName" class="form-control" />
                <span asp-validation-for="ProductOrigin.OriginName" class="text-danger"></span>
            </div>
            <div class="form-group mb-4">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
